  let currentProduct = data.product;\n\n              return <Wrapper>\n                <SideImgContainer>\n                  <SideWrapper>\n                    <SideList gallery={gallery} tab={this.handleTab} myRef={this.myRef} />\n                  </SideWrapper>\n                </SideImgContainer>\n                <ProductImg>\n                  <Image_ src={gallery[index]} alt=\"\" />\n                </ProductImg>\n                <ProductInfo>\n                  <Brand>{brand}</Brand>\n                  <Name>{name}</Name>\n                  <AttributesContainer>\n                    {\n                      data.product.attributes.map((item) => (\n                        <Attributes key={item.id} item={item} />\n                      ))\n                    }\n                    <PriceInfo>\n                      <AttributePrice>PRICE :</AttributePrice>\n                      <AttributePrice>\n                        {prices[0].currency.symbol} {prices[0].amount}\n                      </AttributePrice>\n                    </PriceInfo>\n                  </AttributesContainer>\n\n                  {/* <Link to='/cart' style={{ textDecoration: 'none' }}>\n                    <Button onClick={() => this.addToCart(currentProduct)} cartItems={cartItems}>\n                      ADD TO CART\n                    </Button>\n                  </Link> */}\n                  <Button onClick={() => this.addToCart(currentProduct)} >\n                    ADD TO CART\n                  </Button>\n                  {console.log(cartItems)}\n                  <Hide>\n                    <Cart cartItems={cartItems} />\n                  </Hide>\n                  <ProductDescription><div dangerouslySetInnerHTML={{ __html: description }} /></ProductDescription>\n                </ProductInfo>\n              </Wrapper>\n            }\n          }\n        </Query>\n      </Container>\n    )\n  }\n}\n\nexport default Product\n\nconst Container = styled.div`\n  display: flex;\n